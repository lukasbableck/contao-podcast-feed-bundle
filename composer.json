{
	"name": "lukasbableck/contao-podcast-feed-bundle",
	"type": "contao-bundle",
	"description": "This bundle provides a page type that generates a podcast rss feed from news articles.",
	"keywords": [
		"news",
		"rss",
		"feed",
		"podcast"
	],
	"license": "LGPL-3.0-or-later",
	"authors": [
		{
			"name": "Lukas Bableck",
			"email": "lukas@bableck.dev",
			"homepage": "https://bableck.dev"
		}
	],
	"require": {
		"php": "^8.1",
		"contao/core-bundle": "^5.0",
		"contao/news-bundle": "^5.0"
	},
	"conflict": {
		"contao/manager-plugin": "<2.0 || >=3.0"
	},
	"require-dev": {
		"contao/manager-plugin": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"LukasBableck\\ContaoPodcastFeedBundle\\": "src/"
		}
	},
	"extra": {
		"contao-manager-plugin": "LukasBableck\\ContaoPodcastFeedBundle\\ContaoManager\\Plugin"
	}
}